{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "func-names": 0,
    "indent": [2, 2],
    "indent-size": [true, 2],
    "keyword-spacing": 0,
    "max-len": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "one-var": 0,
    "prefer-arrow-callback": 0,
    "semi": 0,
    "space-before-blocks": 0,
    "space-before-function-paren": 0,
    "spaced-comment": 0,
    "object-curly-spacing": 0
  }
}
